# ============================================================================
# .gitignore for iOS Development with XcodeGen
# ============================================================================

# ----------------------------------------------------------------------------
# Xcode
# ----------------------------------------------------------------------------
# XcodeGen regenerates the project file, so it should not be committed
*.xcodeproj
*.xcworkspace
xcuserdata/
*.xcscmblueprint

# ----------------------------------------------------------------------------
# Build
# ----------------------------------------------------------------------------
build/
DerivedData/

# ----------------------------------------------------------------------------
# Swift Package Manager
# ----------------------------------------------------------------------------
.build/
.swiftpm/
Package.resolved

# ----------------------------------------------------------------------------
# Fastlane
# ----------------------------------------------------------------------------
fastlane/report.xml
fastlane/Preview.html
fastlane/screenshots/*.html
fastlane/screenshots/README.md
fastlane/test_output/

# ----------------------------------------------------------------------------
# CocoaPods (legacy)
# ----------------------------------------------------------------------------
Pods/

# ----------------------------------------------------------------------------
# macOS
# ----------------------------------------------------------------------------
.DS_Store
*.swp

# ----------------------------------------------------------------------------
# IDE
# ----------------------------------------------------------------------------
.idea/
*.code-workspace

# ----------------------------------------------------------------------------
# Claude Code
# ----------------------------------------------------------------------------
.claude/worktrees/
.claude/settings.local.json

# ----------------------------------------------------------------------------
# Firebase Cloud Functions
# ----------------------------------------------------------------------------
functions/lib/
functions/node_modules/

# ----------------------------------------------------------------------------
# Firebase Emulator
# ----------------------------------------------------------------------------
firebase-debug.log
firestore-debug.log
ui-debug.log

# ----------------------------------------------------------------------------
# Environment / User overrides
# ----------------------------------------------------------------------------
.env
*.xcconfig
